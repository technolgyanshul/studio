
{
  "manifest_version": 3,
  "name": "Tab Integrator",
  "version": "1.0",
  "description": "Integrate browser session data with your codebase.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "icons": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
  }
}
