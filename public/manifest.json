{
  "manifest_version": 3,
  "name": "Tab Integrator",
  "version": "1.0",
  "description": "A Next.js Chrome extension to manage and export browser tabs.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
